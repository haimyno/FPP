Name: Starter Workflow Demo

on:
  push:
    branches: 
      - main
  pull_request:
    branches: [ $default-branch ]
  commit:
    branches: [ $default-branch ]
jobs:
  build:
    runs-on: ubuntu-latest

    steps:
      - uses: actions/checkout@v3

      - name: demo workflow job
        run: echo This is a demo start workflow
